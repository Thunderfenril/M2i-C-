<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:RGBMAUI"
             x:Class="RGBMAUI.Detail"
             x:DataType="local:DetailVM"
             Title="Detail">
    <VerticalStackLayout>
        <Label 
            Text ="{Binding ValueRed}" />
        <Label 
            Text="{Binding ValueGreen}" />
        <Label 
            Text="{Binding ValueBlue}"/>
    </VerticalStackLayout>
</ContentPage>